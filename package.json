{
  "name": "matchfind-mvp",
  "private": true,
  "homepage": "https://yashasvi9199.github.io/MatchFind",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "lint": "eslint .",
    "android:sync": "npx cap sync android",
    "android-debug": "BUILD_FOR_ANDROID=true npm run build && npx cap sync android && cd android && ./gradlew assembleDebug",
    "android-release": "BUILD_FOR_ANDROID=true npm run build && npx cap sync android && cd android && ./gradlew assembleRelease",
    "adb-debug": "adb install android/app/build/outputs/apk/debug/app-debug.apk",
    "adb-release": "adb install android/app/build/outputs/apk/release/app-release.apk"
  },
  "dependencies": {
    "@capacitor/android": "^8.0.0",
    "@capacitor/app": "^8.0.0",
    "@capacitor/cli": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@supabase/supabase-js": "^2.87.1",
    "lucide-react": "^0.561.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.25",
    "gh-pages": "^6.3.0",
    "globals": "^16.5.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.49.0",
    "vite": "^6.2.0"
  }
}
